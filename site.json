---
    layout: nil
    ---

    [
    {% for post in site.posts %}
        {
          "title"    : "{{ post.title }}",
          "url"     : "{{ post.url }}",
          "color" : "{{post.color}}",
          "pic1" : "{{post.pic1}}",
          "date"     : "{{ post.date | date: "%B %d, %Y" }}",
          "content"  : "{{ post.content | escape }}",
          "content1" : "{{post.content1}}"
        } {% if forloop.last %}{% else %},{% endif %}
    {% endfor %}
    ]
